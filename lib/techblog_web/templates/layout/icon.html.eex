                <g id="Group" transform="translate(0.000000, -3.000000)">
                    <rect id="Rectangle" x="0" y="44" width="200" height="25" rx="8"></rect>
                    <rect id="Rectangle" transform="translate(165.216199, 34.476593) rotate(38.000000) translate(-165.216199, -34.476593) " x="125.216199" y="21.9765934" width="80" height="25" rx="8"></rect>
